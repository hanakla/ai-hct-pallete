        "@types/react-dom": "^17.0.1",
        "@types/styled-components": "^5.1.24",
        "babel-plugin-styled-components": "^2.0.6",
        "babel-preset-env": "^1.7.0",
        "babel-preset-react": "^6.24.1",
        "babel-preset-typescript": "^7.0.0-alpha.19",
        "npm-run-all": "^4.1.5",
