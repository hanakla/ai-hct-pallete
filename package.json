{
    "private": true,
    "name": "hct-pallet",
    "version": "0.0.1",
    "engines": {
        "node": ">= 10"
    },
    "scripts": {
        "dev": "run-p dev:*",
        "dev:host": "parcel watch --no-hmr --public-url ./ -d ./dist/host src/host/hostscript.ts",
        "dev:client": "parcel watch --hmr-hostname=localhost --public-url ./ -d ./dist/client src/client/index.html",
        "sym": "ln -sf $(pwd) ~/Library/Application\\ Support/Adobe/CEP/extensions/hct-pallet"
    },
